{
  "env": {
    "browser": true
  },
  "extends": ["airbnb/base", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "ignorePatterns": ["dist", "node_modules"],
  "globals": {
    "mQuery": true,
    // "grapesjs": true,
    "Mousetrap": true,
    "Mautic": true,
    "mauticAjaxCsrf": true,
    "MauticVars": true,
    "mauticBaseUrl": false,
    "mauticEditorFonts": true
  }
}
